
<% @questions.each do |question|  %>

<div class="card w-75">
  <div class="card-header">
    <%= question.description%>
  </div>
  <div class="card-body">
    <ul>
      <%question.answers.each do |answer|%>
        <li><%=answer.description%></li>
      <%end%>
    </ul>
  </div>
</div>  
<%end%>


